{
  "name": "khs1994/curl",
  "description": "curl SDK",
  "keywords": [
    "curl",
    "sdk"
  ],
  "homepage": "https://github.com/khs1994-php/curl",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "khs1994",
      "email": "khs1994@khs1994.com",
      "homepage": "https://khs1994.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Curl\\": "src/Curl/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Curl\\Tests\\": "tests/Curl/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    }
  }
}
